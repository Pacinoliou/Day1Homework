@model IEnumerable<Day1Homework.Models.AccountBook>
@(Layout = null)

    <div class="row">
        <div class="col-md-12">
            <table class="table table-bordered table-hover">
                <tr>
                    <th>#</th>
                    <th>類別</th>
                    <th>日期</th>
                    <th>金額</th>
                </tr>
                @{
                        for (int i = 0; i <  Model.Count(); i++)
                        {
                            @Html.Action("GridChildView", new { objSeqNo = i + 1, objAccountBook = Model.ElementAt(i) });
                        }
                    }
            </table>
        </div>
    </div>
